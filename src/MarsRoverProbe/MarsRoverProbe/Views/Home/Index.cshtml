@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Rover Photos Downloader</h1>

    <button id="downloadphotosbutton" class="btn btn-lg btn-outline-primary">Start Download</button>

    <hr />

    <div id="carouselMarsPhotos" class="carousel slide pointer-event" data-ride="carousel" style="">
        <div class="carousel-inner">
        </div>
        <a class="carousel-control-prev" href="#carouselMarsPhotos" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselMarsPhotos" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <div id="logList" style="">
        <label>Ready!</label>
    </div>

</div>